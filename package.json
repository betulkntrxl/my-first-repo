{
  "name": "mt-openai-chat-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@mui/system": "^5.15.15",
    "@preact/signals-react": "^1.3.8",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^17.0.14",
    "prop-types": "^15.8.1",
    "axios": "^1.6.8",
    "axios-retry": "^4.1.0",
    "gpt-tokenizer": "^2.1.2",
    "i18next": "^23.11.3",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.1",
    "react": "^17.0.2",
    "react-device-detect": "^2.2.3",
    "react-dom": "^17.0.2",
    "react-i18next": "^14.1.1",
    "typescript": "^5.4.5",
    "react-router-dom": "^6.23.0",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.2"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "cloud-build": "DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false react-scripts build",
    "test": "DISABLE_ESLINT_PLUGIN=true react-scripts test --transformIgnorePatterns 'node_modules/?!(gpt-tokenizer)/'",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext .ts,.tsx src/",
    "lint:check": "eslint --ext .ts,.tsx src/",
    "prepare": "husky install",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "prettier:check": "prettier --check 'src/**/*.{ts,tsx}'",
    "prod-server": "yarn build && node server/server.js",
    "fake-server": "json-server --watch fake-data/db.json --routes fake-data/routes.json --port 3001",
    "dev-server": "concurrently \"yarn fake-server\" \"yarn start\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "!**/node_modules/**",
      "!build/**",
      "!src/**/*.stories.tsx",
      "!src/**/*.tsx.snap",
      "!src/index.tsx",
      "!src/reportWebVitals.ts"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "proxy": "http://localhost:3001",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/runtime": "^7.24.1",
    "@types/react-test-renderer": "^17.0.2",
    "@types/use-sync-external-store": "^0.0.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^17.0.2",
    "stylelint-config-prettier": "^9.0.5",
    "webpack": "^5.91.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
